##### Database #####

root:
  contest:
    <contest_id>  
      contest_name: <name>
      creator_username: <username>
      participants:
        <user_id>
          score: <score>
          rank: <rank>      
      message:
         <unique push_id>
            user_id: <user_id>
            user_name: <username>
            text: <text>
            photoUrl: <url>
            nlikes: <num>
            like:
              <user_id> : <0>
            time_stamp: <time> date & time
      
      
  user:
    <user_id>
      user_name: <name>
      no_plant: <num>
      plants:
        <plant_id> : <value 0>     
      contests:
        <contest_id>  
          name: <name>
      post:
        <post_id> : <value 0>
     
  
  plant:
    <curruser_id>
      id:
      name:
      photoUrl:
      text:
      
            
  post:
    <post_id>
        user_id: <user_id>
        user_name: <name>
        message: <text>
        image: <url>
        timestamp: <date & time>
        nlikes: <num>
        like:
          <user_id> : <0>
        ncomment: <num>
        comment:
          <unique_push_id>
            user_id: <user id who commented>
            user_name: <name>
            content: <text>
